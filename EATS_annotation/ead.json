{
    "$schema": "https://gitlab.com/empaia/integration/definitions/-/raw/main/ead/ead-schema.v3.json",
    "name": "My Cool Medical AI Algorithm",
    "name_short": "Cool App",
    "namespace": "org.empaia.helse_vest_piv.cool_app.v3.1",
    "description": "Does super advanced AI stuff, you know...",
    "classes": {
        "pixel": {
            "name": "Red pixel"
        }
    },

    "io": {
        "my_wsi": {
            "type": "wsi"
        },
        "my_rectangle": {
            "type": "rectangle",
            "reference": "io.my_wsi",
            "classes": [
                "org.empaia.global.v1.classes.roi"
            ]
        },
        "my_quantification_result": {
            "type": "float"
        },
        "fibrosis_score": {
            "type": "float",
            "reference": "io.my_rectangle"
        },
        "my_pixels": {
            "type": "collection",
            "items": {
                "type": "collection",
                "reference": "io.my_rectangle",
                "items": {
                    "type": "point",
                    "reference": "io.my_wsi"
                }
            }
        },
        "my_pixel_classes": {
            "type": "collection",
            "items": {
                "type": "collection",
                "items": {
                    "type": "class",
                    "reference": "io.my_pixels.items.items"
                }
            }
        }
    },
    "modes": {
        "standalone": {
            "inputs": [
                "my_wsi",
                "my_rectangle"
            ],
            "outputs": [
                "my_quantification_result",
                "fibrosis_score",
                "my_pixels",
                "my_pixel_classes"
            ]
        }
    }
}